{
  "index_name": "symfony",
  "start_urls": [
    {
      "url": "https://symfony.com/doc/(?P<version>.*?)/",
      "variables": {
        "version": {
          "url": "https://symfony.com/doc/current/book/controller.html",
          "js": "var versions = $('.doc-switcher .version, .doc-switcher li a').map(function (i, elt) { return $(elt).text().trim(); }).filter(function(i, e) { return e.indexOf('master') === -1 && e.indexOf('version') === -1; }).toArray(); versions.push('current'); versions.push('master'); return JSON.stringify(versions);"
        }
      }
    },
    "https://symfony.com/doc/",
    "https://symfony.com/doc/current/index.html"
  ],
  "stop_urls": [
    "index\\.html",
    "^(?!.*html)"
  ],
  "scrap_start_urls": false,
  "selectors_exclude": [],
  "selectors": {
    "lvl0": {
      "selector": "//*[contains(@class, 'panel-body')]/ul/li[contains(@class, 'selected')]/../../../preceding::div[contains(@class, 'panel-heading')][1]/h4",
      "type": "xpath"
    },
    "lvl1": "#main h1",
    "lvl2": "#main h2",
    "lvl3": "#main h3",
    "lvl4": "#main h4",
    "text": "#main p, #main li"
  },
  "min_indexed_level": 2,
  "nb_hits": 6100
}
