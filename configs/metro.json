{
  "index_name": "metro",
  "start_urls": [
    {
      "url": "http://facebook.github.io/metro-bundler/docs/",
    }
  ],
  "selectors_exclude": [
    ".hash-link",
    ".edit-page-link"
  ],
  "selectors": {
    "lvl0": {
      "selector": "//nav//*[contains(@class,'navListItemActive')]/../../h3",
      "type": "xpath",
      "global": true,
      "default_value": "Documentation"
    },
    "lvl1": ".post header h1",
    "lvl2": ".post article h2",
    "lvl3": ".post article h3",
    "lvl4": ".post article h4",
    "lvl5": ".post article h5",
    "text": ".post article p, .post article li"
  },
  "min_indexed_level": 2,
}
