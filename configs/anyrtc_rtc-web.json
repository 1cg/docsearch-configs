{
  "index_name": "anyrtc_rtc-web",
  "start_urls": [
    "https://docs.anyrtc.io/rtc-web/"
  ],
  "sitemap_alternate_links": true,
  "stop_urls": [
    "^((?!\\.html).)*$"
  ],
  "selectors": {
    "lvl0": {
      "selector": ".menu__link--sublist.menu__link--active",
      "global": true,
      "default_value": "Documentation"
    },
    "lvl1": "header h1",
    "lvl2": "article h2",
    "lvl3": "article h3",
    "lvl4": "article h4",
    "lvl5": "article h5, article td:first-child",
    "lvl6": "article h6",
    "text": "article p, article li, article td:last-child"
  },
  "strip_chars": " .,;:#",
  "selectors_exclude": [
    ".header",
    ".footer"
  ],
  "custom_settings": {
    "separatorsToIndex": "_",
    "attributesForFaceting": [
      "language",
      "version",
      "type"
    ],
    "attributesToRetrieve": [
      "hierarchy",
      "content",
      "anchor",
      "url",
      "url_without_anchor",
      "type"
    ]
  },
  "scrape_start_urls": false
}