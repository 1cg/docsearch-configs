{
    "index_name": "atlassianps",
    "start_urls": [
        "https://atlassianps.org/docs/"
    ],
    "stop_urls": [],
    "sitemap_urls": [
        "https://atlassianps.org/sitemap.xml"
    ],
    "sitemap_urls_regexs": "/docs/",
    "selectors": {
        "lvl0": {
            "selector": ".doc-nav ul.onepress-menu >li.menu-item.active >a",
            "global": true
        },
        "lvl1": {
            "selector": ".doc-nav ul.onepress-menu >li >ul.sub-menu >li.menu-item.active >a",
            "default_value": "",
            "global": true
        },
        "lvl2": "section.content h1",
        "lvl3": "section.content h2",
        "lvl4": "section.content h3",
        "lvl5": "section.content h4",
        "text": "section.content p, section.content li"
    },
    "strip_chars": "\u2014\u2013 --",
    "min_indexed_level": 2,
    "nb_hits": 600
}